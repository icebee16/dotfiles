### ZPLUG_HOME
export ZPLUG_HOME=$HOME/.zplug

### Anaconda3
if [ -e "$HOME/anaconda3" ] ; then
		PATH="$HOME/anaconda3/bin:$PATH"
fi

# libclang path for deoplete-clang
export LIB_CLANG="/usr/lib/llvm-6.0/lib/libclang-6.0.so"
export CLANG_HEADER="/usr/lib/llvm-6.0/lib/clang"

### wsl zsh
setopt NO_BG_NICE

### jupyter notebook
alias 'jn'='jupyter notebook --config="/home/icebee/.config/jupyter/jupyter_notebook_config.py"'
